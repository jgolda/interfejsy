<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Cennik" table="CENNIK">
        <id name="id" type="java.lang.Long">
            <column name="CEN_ID"/>
            <generator class="native"/>
        </id>
        <property name="kwota" type="java.lang.Double">
            <column name="CEN_KWOTA"/>
        </property>
        <property name="opis" type="java.lang.String">
            <column name="CEN_OPIS"/>
        </property>
        <set name="uslugi" lazy="true" inverse="true">
            <key column="USL_CEN_ID"/>
            <one-to-many class="model.Usluga"/>
        </set>
    </class>
</hibernate-mapping>