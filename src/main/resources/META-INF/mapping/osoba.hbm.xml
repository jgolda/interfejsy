<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Osoba" table="OSOBY" abstract="true">
        <id name="id" type="java.lang.Long">
            <column name="OSO_ID"/>
            <generator class="native"/>
        </id>
        <property name="imie" type="java.lang.String">
            <column name="OSO_IMIE"/>
        </property>
        <property name="nazwisko" type="java.lang.String">
            <column name="OSO_NAZWISKO"/>
        </property>
        <property name="pesel" type="java.lang.String">
            <column name="OSO_PESEL"/>
        </property>
        <many-to-one name="adres"
                     class="model.Adres"
                     cascade="persist"
                     column="OSO_ADR_ID"
                     unique="true"/>
    </class>
</hibernate-mapping>

